       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="myIoTDeviceBean-1" class="io.mlu.mde.IoTDevice" init-method="initBean" destroy-method="destroyBean"/>
    <bean id="myIoTDeviceBean-2" class="io.mlu.mde.IoTDevice" init-method="initBean" destroy-method="destroyBean"/>

